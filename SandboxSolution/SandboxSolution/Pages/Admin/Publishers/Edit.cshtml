@page
@model SandboxSolution.Pages.Admin.Publishers.EditModel
@{
}
<script src="~/js/updatepublisher.js"></script>
<form id="edit-publisher-form">
	<div class="form-group">
		<label for="publisher-name">Publisher name</label>
		<input type="text" class="form-control col-3" id="publisher-name" placeholder="Publisher name">
	</div>
	<div class="form-group">
		<label for="deleted">IsDeleted</label><br />
		<input type="checkbox" class="col-3" id="deleted">
	</div>
	<br />
	<button type="button" class="btn btn-success" onclick="onClickEdit()">Edit</button>
	<button type="button" class="btn btn-danger" onclick="onClickReset()">Reset</button>
</form>